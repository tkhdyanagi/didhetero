# Load the package
library(didhetero)
# Generate artificial data
set.seed(1)
data <- datageneration(n = 500, tau = 4)
# Uniform inference for CATT
output1 <- catt_gt_continuous(
yname = "Y",
tname = "period",
idname = "id",
gname = "G",
zname = "Z",
xformla = ~ Z,
data = data,
zeval = seq(-1, 1, by = 0.1),
gteval = NULL,
pretrend = FALSE,
control_group = "notyettreated",
anticipation = 0,
alp = 0.05,
bstrap = TRUE,
biters = 1000,
porder = 2,
kernel = "gau",
bwselect = "IMSE1",
bw = NULL,
uniformall = TRUE
)
# Uniform inference for the event-study-type parameter
output2 <- aggte_continuous(
output = output1,
type = "dynamic",
eval = NULL,
bstrap = TRUE,
biters = 1000,
porder = 2,
bwselect = "IMSE1",
bw = NULL,
uniformall = TRUE
)
# Uniform inference for the simple overall parameter
output3 <- aggte_continuous(
output = output1,
type = "simple",
eval = NULL,
bstrap = TRUE,
biters = 1000,
porder = 2,
bwselect = "IMSE1",
bw = NULL,
uniformall = TRUE
)
output1$Figure1$g2_t2
output1$Figure2$g2_t2
output2$Figure1$eval0
output2$Figure2$eval0
